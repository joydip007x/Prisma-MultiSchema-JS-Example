generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("PRISMA_DATABASE_URL")
}

type Moods {
  happy String?
  sad   Int?
}

model User {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  email String @unique
  mood  Moods
}
